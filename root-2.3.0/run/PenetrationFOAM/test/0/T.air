FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T.air;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    aeration
    {
        type            fixedValue;
        value           uniform 300;
    }
    xsides
    {
        type            zeroGradient;
    }
    zsides
    {
        type            zeroGradient;
    }
    outlet
    {
        type            inletOutlet;
        phi             phi.air;
        inletValue      uniform 300;
        value           uniform 300;
    }
    semiConductor1
    {
        type            zeroGradient;
    }
    semiConductor1-shadow
    {
        type            zeroGradient;
    }
    semiConductor2
    {
        type            zeroGradient;
    }
    semiConductor2-shadow2
    {
        type            zeroGradient;
    }
}


